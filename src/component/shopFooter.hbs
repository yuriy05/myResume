<footer class="mt-3">
    <div class="row">
        {{#each footer}}
        <section class="col-1 me-5">
            <ul class="nav">
                {{#each this}}
                <li class="nav-item">
                    {{#if @last}}
                    <a class="nav-link disabled" href="#">{{this.text}}</a>
                    {{else}}
                    <a class="nav-link" href="{{this.link}}">{{this.text}}</a>
                    {{/if}}
                </li>
                {{/each}}
            </ul>
        </section>
        {{/each}}
    </div>
</footer>